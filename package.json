{
  "name": "freakshow-scraper",
  "version": "1.0.0",
  "type": "module",
  "description": "Scraper for Freak Show podcast archive",
  "main": "scripts/scrape.js",
  "scripts": {
    "scrape": "node scripts/scrape.js",
    "scrape-details": "node scripts/scrape-details.js",
    "scrape-chapters": "node scripts/scrape-chapters.js",
    "scrape-osf": "node scripts/scrape-osf.js",
    "scrape-speakers": "node scripts/scrape-speakers.js",
    "extract-topics": "node scripts/extract-topics.js",
    "extend-topics": "node scripts/generate-extended-topics.js --all",
    "normalize-topics": "node scripts/normalize-topics.js",
    "create-embeddings": "node scripts/create-embeddings.js",
    "create-rag-db": "node scripts/create-rag-db.js",
    "cluster-topics": "node scripts/cluster-topics.js",
    "topic-river": "node scripts/generate-topic-river.js",
    "episodes-mp3": "node scripts/generate-episodes-mp3.js",
    "cluster-categories": "node scripts/cluster-categories.js",
    "category-river": "node scripts/generate-category-river.js",
